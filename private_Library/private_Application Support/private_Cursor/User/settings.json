    "better-comments.tags": [
        // Window must be reloaded for these to appear
        // Based on https://draculatheme.com/contribute#color-palette
        // ━━━━━━━━━━━━━━━━━━━━ Color Reference ━━━━━━━━━━━━━━━━━━━ */
        {
            // Background
            "tag": "background",
            "color": "#282A36",
            "italic": true,
            "bold": true,
        },
        {
            // Foreground
            "tag": "foreground",
            "color": "#F8F8F2",
            "italic": true,
            "bold": true,
        },
        {
            // Selection
            "tag": "selection",
            "color": "#44475A",
            "italic": true,
            "bold": true,
        },
        {
            // Comment
            "tag": "comment",
            "color": "#6272A4",
            "italic": true,
            "bold": true,
        },
        {
            // Red
            "tag": "red",
            "color": "#FF5555",
            "italic": true,
            "bold": true,
        },
        {
            // Orange
            "tag": "orange",
            "color": "#FFB86C",
            "italic": true,
            "bold": true,
        },
        {
            // Yellow
            "tag": "yellow",
            "color": "#F1FA8C",
            "italic": true,
            "bold": true,
        },
        {
            // Green
            "tag": "green",
            "color": "#50FA7B",
            "italic": true,
            "bold": true,
        },
        {
            // Purple
            "tag": "purple",
            "color": "#BD93F9",
            "italic": true,
            "bold": true,
        },
        {
            // Cyan
            "tag": "cyan",
            "color": "#8BE9FD",
            "italic": true,
            "bold": true,
        },
        {
            // Pink
            "tag": "pink",
            "color": "#FF79C6",
            "italic": true,
            "bold": true,
        },
        // ━━━━━━━━━━━━━━━━━━━━━━━━ Actual ━━━━━━━━━━━━━━━━━━━━━━━━
        {
            // %% Jupyter Cell
            "tag": "%%",
            "color": "#50FA7B", // Green
            "italic": true,
            "bold": true,
        },
        {
            // !! Warning
            "tag": "!!",
            "color": "#FF5555", // Red
            "italic": true,
            "bold": true,
        },
        {
            // Warning
            "tag": "warning",
            "color": "#FF5555", // Red
            "italic": true,
            "bold": true,
        },
        {
            // ?? Question
            "tag": "??",
            "color": "#FF79C6", // Purple
            "italic": true,
            "bold": true,
        },
        {
            // ?? Question
            "tag": "question",
            "color": "#FF79C6", // Purple
            "italic": true,
            "bold": true,
        },
        {
            // Info
            "tag": "info",
            "color": "#8BE9FD", // Blue
            "italic": true,
            "bold": true,
        },
        {
            // //Strikethrough
            "tag": "//",
            "color": "#6272A4", // Comment
            "strikethrough": true,
            "italic": true,
        },
        {
            // Fixme
            "tag": "fixme",
            "color": "#FFB86C", // Orange
            "italic": true,
            "bold": true,
        },
        {
            // ━━━━━━━━━━━━━━━━━━━━━━ Header ━━━━━━━━━━━━━━━━━━━━━━
            "tag": "━",
            "color": "#F8F8F2", // Foreground
            "italic": true,
            "bold": true,
        },
        {
            // ───────────────────── Subheader ────────────────────
            "tag": "─",
            "color": "#F8F8F2", // Foreground
            "italic": true,
            "bold": true,
        },
        {
            // # Sub-Subheader
            "tag": "#",
            "color": "#F8F8F2", // Foreground
            "italic": true,
            "underline": true,
        }
    ],
