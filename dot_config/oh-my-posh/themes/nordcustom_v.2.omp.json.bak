{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "type": "text",
            "style": "plain",
            "foreground": "#E5E9F0",
            "template": "\u256d\u2500"
          },
          // {
          //   "type": "text",
          //   "style": "plain",
          //   // "foreground": "#E5E9F0",
          //   // "background": "#E5E9F0",
          //   "template": "î‚¶ðŸª¿î‚´"
          // },
          {
            "type": "session",
            "background": "#E5E9F0",
            "foreground": "#4C566A",
            "leading_diamond": "\ue0b6",
            "style": "diamond",
            "template":"{{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }} "
          },

          {
            "type": "aws",
            "style": "powerline",
            "powerline_symbol": "î‚°",
            "foreground": "#ffffff",
            "background": "#FFA400"
          },
          {
            "type": "docker",
            "style": "powerline",
            "powerline_symbol": "î‚°",
            "foreground": "#000000",
            "background": "#0B59E7"
          },
          {
            "type": "kubectl",
            "style": "powerline",
            "powerline_symbol": "î‚°",
            "foreground": "#000000",
            "background": "#ebcc34",
            "template": " ï´± {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
            "properties": {
              "context_aliases": {
                "arn:aws:eks:eu-west-1:1234567890:cluster/posh": "posh"
              }
            }
          },
          
          {
            "type": "path",
            "style": "powerline",
            "background": "#81A1C1",
            "foreground": "#2E3440",
            "powerline_symbol": "\ue0b0",
            "properties": {
              "style": "mixed",
              "home_icon": "~",
              "cycle": ["#2E3440", "#3B4252", "#434C5E", "#4C566A"],
              "cycle_folder_separator": true,
              "gitdir_format": "%s",
              "edge_format": "%s"
            }
          },
          {
            "type": "python",
            "style": "powerline",
            "powerline_symbol": "î‚°",
            "foreground": "#100e23",
            "background": "#906cff",
            "template": " ó°Œ  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }} {{ end }}"
          },
          {
            "type": "project",
            "style": "powerline",
            "powerline_symbol": "î‚°",
            "foreground": "#193549",
            "background": "#ffeb3b",
            "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}\uf487 {{ end }} {{ if .Name }}{{ .Name }}{{ end }}{{ end }}"
          },
          {
            "type": "git",
            "background": "#A3BE8C",
            "background_templates": [
              "{{ if or (.Working.Changed) (.Staging.Changed) }}#D08770{{ end }}",
              "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#EBCB8B{{ end }}",
              "{{ if gt .Ahead 0 }}#B48EAD{{ end }}",
              "{{ if gt .Behind 0 }}#B48EAD{{ end }}"
            ],
            "foreground": "#2E3440",
            "powerline_symbol": "\ue0b0",
            "properties": {
              "fetch_stash_count": true,
              "fetch_status": true,
              "fetch_upstream_icon": true
            },
            "style": "powerline",
            "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} "
          },
          {
            "background": "#B48EAD",
            "foreground": "#2E3440",
            "powerline_symbol": "\ue0b0",
            "properties": {
              "always_enabled": true
            },
            "style": "powerline",
            "template": " \ueba2 {{ .FormattedMs }} ",
            "type": "executiontime"
          },
          {
            "type": "status",
            "background": "#EBCB8B",
            "background_templates": [
              "{{ if gt .Code 0 }}#f1184c{{ end }}"
            ],
            "foreground": "#2E3440",
            "properties": {
              "always_enabled": true
            },
            "style": "diamond",
            "template": " {{ if gt .Code 0 }}\uf00d {{ .Code }} {{ reason .Code }}{{ else }}\uf42e {{ end }}",
            "trailing_diamond": "\ue0b0"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "right",
        "segments": [
          {
            "background": "#D08770",
            "foreground": "#2E3440",
            "invert_powerline": true,
            "powerline_symbol": "\ue0b2",
            "style": "powerline",
            "template": "\uf489 {{ .Name }} </>",
            "type": "shell"
          },
          {
            "background": "#81A1C1",
            "foreground": "#2E3440",
            "invert_powerline": true,
            "trailing_diamond": "\ue0b4",
            "style": "diamond",
            "template": " \ue641 {{ .CurrentDate | date .Format }} ",
            "type": "time"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#ECEFF4",
            "style": "plain",
            "template": "\u2570\u2500",
            "type": "text"
          },
          {
            "foreground": "#8FBCBB",
            "foreground_templates": [
              "{{ if gt .Code 0 }}#ef5350{{ end }}"
            ],
            "properties": {
              "always_enabled": true
            },
            "style": "plain",
            "template": "<#B48EAD>\u276f</><#A3BE8C>\u276f</><#EBCB8B>\u276f</> ",
            "type": "status"
          }
        ],
        "type": "prompt"
      }
    ],
    "secondary_prompt": {
      "background": "transparent",
      "foreground": "#ffffff",
      "template": "-> "
  },
  "console_title_template": "{{.Folder}}{{if .Root}} :: root{{end}} :: {{.Shell}}",
    "version": 2
  }