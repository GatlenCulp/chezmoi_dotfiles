{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "secondary_prompt": {
    "template": "-> ",
    "foreground": "#ffffff",
    "background": "transparent"
  },
  "console_title_template": "{{.Folder}}{{if .Root}} :: root{{end}} :: {{.Shell}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u256d\u2500",
          "foreground": "#E5E9F0",
          "type": "text",
          "style": "plain"
        },
        {
          "leading_diamond": "\ue0b6",
          "template": "{{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }} ",
          "foreground": "#4C566A",
          "background": "#E5E9F0",
          "type": "session",
          "style": "diamond"
        },
        {
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#FFA400",
          "type": "aws",
          "style": "powerline"
        },
        {
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#0B59E7",
          "type": "docker",
          "style": "powerline"
        },
        {
          "properties": {
            "context_aliases": {
              "arn:aws:eks:eu-west-1:1234567890:cluster/posh": "posh"
            }
          },
          "template": " \ufd31 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#ebcc34",
          "type": "kubectl",
          "style": "powerline"
        },
        {
          "properties": {
            "cycle": [
              "#2E3440",
              "#3B4252",
              "#434C5E",
              "#4C566A"
            ],
            "cycle_folder_separator": true,
            "edge_format": "%s",
            "gitdir_format": "%s",
            "home_icon": "~",
            "style": "mixed"
          },
          "foreground": "#2E3440",
          "powerline_symbol": "\ue0b0",
          "background": "#81A1C1",
          "type": "path",
          "style": "powerline"
        },
        {
          "template": " \udb80\udf20 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }} {{ end }}",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#906cff",
          "type": "python",
          "style": "powerline"
        },
        {
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}\uf487 {{ end }} {{ if .Name }}{{ .Name }}{{ end }}{{ end }}",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "background": "#ffeb3b",
          "type": "project",
          "style": "powerline"
        },
        {
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#2E3440",
          "powerline_symbol": "\ue0b0",
          "background": "#A3BE8C",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#D08770{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#EBCB8B{{ end }}",
            "{{ if gt .Ahead 0 }}#B48EAD{{ end }}",
            "{{ if gt .Behind 0 }}#B48EAD{{ end }}"
          ]
        },
        {
          "properties": {
            "always_enabled": true
          },
          "template": " \ueba2 {{ .FormattedMs }} ",
          "foreground": "#2E3440",
          "powerline_symbol": "\ue0b0",
          "background": "#B48EAD",
          "type": "executiontime",
          "style": "powerline"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "trailing_diamond": "\ue0b0",
          "template": " {{ if gt .Code 0 }}\uf00d {{ .Code }} {{ reason .Code }}{{ else }}\uf42e {{ end }}",
          "foreground": "#2E3440",
          "background": "#EBCB8B",
          "type": "status",
          "style": "diamond",
          "background_templates": [
            "{{ if gt .Code 0 }}#f1184c{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "template": "\uf489 {{ .Name }} </>",
          "foreground": "#2E3440",
          "powerline_symbol": "\ue0b2",
          "background": "#D08770",
          "type": "shell",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "trailing_diamond": "\ue0b4",
          "template": " \ue641 {{ .CurrentDate | date .Format }} ",
          "foreground": "#2E3440",
          "background": "#81A1C1",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u2570\u2500",
          "foreground": "#ECEFF4",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "template": "<#B48EAD>❯</><#A3BE8C>❯</><#EBCB8B>❯</> ",
          "foreground": "#8FBCBB",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
